<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">


	<navigation-rule>
		<from-view-id>/*</from-view-id>

		<navigation-case>
			<from-outcome>user</from-outcome>
			<to-view-id>pages/user.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>license_terms</from-outcome>
			<to-view-id>pages/licenseTerms.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>pages/home.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>pages/login.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>events</from-outcome>
			<to-view-id>pages/events.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>registration</from-outcome>
			<to-view-id>pages/registration.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>news</from-outcome>
			<to-view-id>pages/news.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>projects</from-outcome>
			<to-view-id>pages/projects.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>team</from-outcome>
			<to-view-id>pages/team.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>research</from-outcome>
			<to-view-id>pages/research.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>repository</from-outcome>
			<to-view-id>pages/repository.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>

	<navigation-rule>	<!-- Event navigation rule -->
		<from-view-id>events.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>event</from-outcome>
			<to-view-id>pages/event.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
